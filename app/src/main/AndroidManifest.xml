<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhekai.universityvenuemanagementsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mmu_logo_launcher_background"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mmu_logo_launcher_background_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UniversityVenueManagementSystem">
        <activity
            android:name=".SearchActivity"
            android:exported="false" />
        <activity
            android:name=".ViewReservationActivity"
            android:exported="true" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ReservationActivity"
            android:exported="true" />
        <activity
            android:name=".LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>